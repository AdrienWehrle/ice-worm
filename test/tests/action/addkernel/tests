[Tests]
  [./noaction]
    # 1 element problem with no MASTODON actions
    type = Exodiff
    input = noaction.i
    exodiff = noaction_out.e
  [../]

  [./inertia_kernel_action]
    # 1 element problem with action that automatically creates inertia Kernels
    type = Exodiff
    input = inertia_kernel_action.i
    exodiff = noaction_out.e
    cli_args = "Outputs/file_base=noaction_out"
    prereq = noaction
  [../]

  [./auxkernel_action]
    # 1 element problem with action that automatically creates velocity and acceleration AuxVariables
    type = Exodiff
    input = auxkernel_action.i
    exodiff = noaction_out.e
    cli_args = "Outputs/file_base=noaction_out"
    prereq = inertia_kernel_action
  [../]
